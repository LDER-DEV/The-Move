<%- include('partials/header') -%>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
    <div class="container">
      <a class="navbar-brand" href="/">
        <img src="/img/nav logo.png" class="navLogo" alt="Logo">
      </a>

      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-collapse-1"
        aria-controls="navbar-collapse-1" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbar-collapse-1">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item">
            <a class=" btn btn-outline-light" href="/">Home</a>
          </li>

          <li class="nav-item">
            <a class=" btn btn-outline-light" href="/profile/<%= user._id%>">Profile</a>
          </li>
          <li class="nav-item">
            <a class=" btn btn-outline-light" href="/venues">Venues</a>
          </li>
          <li class="nav-item">
            <a class=" btn btn-outline-light" href="/upcomingMoves">Moves</a>
          </li>
          <li class="nav-item">
            <a class=" btn btn-outline-light" href="/logout">Logout</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>

  <div class="container">
    <h1>Artists</h1>
    <div class="row justify-content-center mt-5">
      <ul class="userCont list-unstyled">

        <% for(var i=0; i < profile.length; i++){%>
          <% if(profile[i].isVenue==='false' ){%>
            <li>
              <a href="/profile/<%= profile[i].id%>">
                <h1>
                  <%= profile[i].userName%>
                </h1>
              </a>
              <div class="row-4 justify-content-between mt-5">
                <section class="imageContainer">

                  <a href="/profile/<%= profile[i].id%>">
                    <img class="img-fluid profileImage" src="<%= profile[i].profilePicture%>">
                  </a>

                </section>
              </div>
            </li>
            <% }} %>
      </ul>
    </div>
  </div>
  <%- include('partials/footer') -%>